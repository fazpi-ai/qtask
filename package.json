{
  "name": "@fazpi-ai/qtask",
  "version": "1.0.6",
  "description": "A Redis-based queue processing library for Node.js, designed for efficient background job processing.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && mkdir -p dist/scripts && cp -r src/scripts/*.lua dist/scripts/",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "example": "ts-node examples/basic-example.ts",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "queue",
    "job",
    "task",
    "worker",
    "redis",
    "bull"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "generic-pool": "^3.9.0",
    "ioredis": "^5.3.2",
    "rimraf": "^5.0.10",
    "typescript": "^5.8.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/generic-pool": "^3.1.9",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
